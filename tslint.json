{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "ban-types": false,
    "callable-types": false,
    "class-name": false,
    "max-classes-per-file": false,
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "ordered-imports": true,
    "variable-name": false
  }
}
