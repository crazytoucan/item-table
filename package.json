    "react": ">=16.13",
    "react-dom": ">=16.13",
    "shallowequal": "^1.1.0",
    "tinysaga": "^1.3.2",
    "tslib": "^2.0.1",
    "uuid": "^8.3.0"
  },
  "private": true,
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/resize-observer-browser": "^0.1.3",
    "fuse-box": "^4.0.0-next.444",
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
